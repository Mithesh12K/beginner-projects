import random

random_number = random.randint(1, 100)
while True:
  guess_str = input("Guess the NUMBER between 1 and 100: ")
  guess = int(guess_str)
  
  if guess < random_number:
    print("Too low!")
  elif guess > random_number:
    print("Too high!")
  else:
    print("Congratulations! You guessed the number.")
    break
